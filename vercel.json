{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "dest": "api/app.py"
    }
  ],
  "functions": {
    "api/app.py": {
      "runtime": "python@3.9"
    }
  },
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
