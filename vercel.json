{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "handler": "app.app"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}