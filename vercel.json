# Open vercel.json and replace content with:
{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "app.py"
    }
  ]
}